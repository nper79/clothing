# Replicate API Key (for image generation)
# Get your API key from: https://replicate.com/account
VITE_REPLICATE_API_TOKEN=your_replicate_api_token_here

# Google Gemini API Key (for other AI features)
# Get your API key from: https://aistudio.google.com/app/apikey
VITE_API_KEY=your_gemini_api_key_here

# Firebase Configuration
# Get your config from Firebase Console
VITE_FIREBASE_API_KEY=your_firebase_api_key_here
VITE_FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your_project_id
VITE_FIREBASE_STORAGE_BUCKET=your_project.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=your_sender_id
VITE_FIREBASE_APP_ID=your_app_id

# Supabase Configuration
# Get your config from Supabase Dashboard
VITE_SUPABASE_URL=your_supabase_url_here
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Optional: Supabase Service Role Key (for server-side operations)
# SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# App Configuration
VITE_APP_NAME=Clothing AI Style Assistant
VITE_APP_VERSION=1.0.0

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_DEBUG=false